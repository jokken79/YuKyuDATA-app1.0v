{
  "name": "yukyudata-app1.0v",
  "version": "1.0.0",
  "description": "Sistema completo para la gestion de vacaciones pagadas de empleados, desarrollado con FastAPI + SQLite + JavaScript vanilla.",
  "main": "cache-manager.js",
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage",
    "build": "npm run build:js && npm run build:css",
    "build:js": "node scripts/build-assets.js --js",
    "build:css": "node scripts/build-assets.js --css",
    "build:bundle": "node scripts/build-assets.js --bundle",
    "build:watch": "node scripts/build-assets.js --watch",
    "build:clean": "node scripts/build-assets.js --clean",
    "prebuild": "npm run build:clean",
    "lint:js": "eslint static/js/**/*.js --ignore-pattern '*.min.js'",
    "lint:css": "stylelint static/css/**/*.css --ignore-pattern '*.min.css'",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jokken79/YuKyuDATA-app1.0v.git"
  },
  "keywords": [
    "yukyu",
    "vacation-management",
    "fastapi",
    "japanese-labor-law"
  ],
  "author": "YuKyuDATA Team",
  "license": "ISC",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/jokken79/YuKyuDATA-app1.0v/issues"
  },
  "homepage": "https://github.com/jokken79/YuKyuDATA-app1.0v#readme",
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@storybook/addon-a11y": "^8.5.0",
    "@storybook/addon-essentials": "^8.5.0",
    "@storybook/addon-links": "^8.5.0",
    "@storybook/blocks": "^8.5.0",
    "@storybook/html": "^8.5.0",
    "@storybook/html-vite": "^8.5.0",
    "babel-jest": "^30.2.0",
    "chokidar": "^3.6.0",
    "cssnano": "^6.1.2",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.0",
    "storybook": "^8.5.0",
    "terser": "^5.46.0",
    "vite": "^6.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
