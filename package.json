{
  "name": "yukyudata-app1.0v",
  "version": "1.0.0",
  "description": "Sistema completo para la gestion de vacaciones pagadas de empleados, desarrollado con FastAPI + SQLite + JavaScript vanilla.",
  "main": "cache-manager.js",
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:a11y": "jest --testPathPattern=accessibility",
    "build": "webpack --mode production",
    "build:dev": "webpack --mode development",
    "build:watch": "webpack --watch --mode development",
    "build:analyze": "webpack-bundle-analyzer dist/stats.json",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint static/src/**/*.js --fix",
    "lint:css": "stylelint static/css/**/*.css --fix",
    "start:test": "python -m uvicorn main:app --host 0.0.0.0 --port 8000",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prebuild": "npm run lint && npm run test:coverage",
    "dev": "webpack serve --mode development",
    "bundle-analyze": "webpack-bundle-analyzer dist/stats.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jokken79/YuKyuDATA-app1.0v.git"
  },
